<% include ../partials/header %>
<div class="row">
    <h1>Your search results are shown below</h1>
    <div class="row"><br>
  <div class="grid-container">
      <% res1.forEach(function(product) { %>    
      <box><div class="item">     
                <div class="item">
                   <h1 class="itemp"><%= product.title %></h1>
                   <h1 class="itemp">Brand: <%= product.brand %></h1>
                   <image src ="/<%= product.image %>" class="pimage"></image>
                   <h1 class="itemp">Genre: <%= product.genre %></h1>
                   <h1 class="itemp">Description: <%= product.description %></h1>
                   <h1 class="itemp">Quality: <%= product.quality %></h1>
                   <h1 class="itemp">Price: <%= product.price %></h1>
                   
                </div>
            </div></box>
         <% }); %>
    </div>
  </h1>
</div>
<% include ../partials/footer %>
</body>
</html>